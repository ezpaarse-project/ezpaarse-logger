{
  "name": "ezPAARSE Logger (App)",
  "version": "1.2.2",
  "description": "Analyzes the web traffic captured by the logger extension with ezPAARSE.",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": ["storage"],
  "icons": {
    "16": "img/ezlogger-16.png",
    "48": "img/ezlogger-48.png",
    "128": "img/ezlogger-128.png"
  }
}
